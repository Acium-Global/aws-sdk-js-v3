{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "DOM",
      "es5",
      "es2015.collection",
      "es2015.iterable",
      "es2015.promise"
    ],
    "downlevelIteration": true,
    "noImplicitUseStrict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "inlineSources": true,
    "inlineSourceMap": true
  }
}
